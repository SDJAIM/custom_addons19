<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Insurance Claim Sequence -->
        <record id="sequence_clinic_insurance_claim" model="ir.sequence">
            <field name="name">Insurance Claim</field>
            <field name="code">clinic.insurance.claim</field>
            <field name="prefix">CLM/%(year)s/</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Payment Plan Sequence -->
        <record id="sequence_clinic_payment_plan" model="ir.sequence">
            <field name="name">Payment Plan</field>
            <field name="code">clinic.payment.plan</field>
            <field name="prefix">PP/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>