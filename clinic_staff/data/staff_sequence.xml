<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Staff Sequences -->
        <record id="seq_clinic_staff_doctor" model="ir.sequence">
            <field name="name">Clinic Doctor Code</field>
            <field name="code">clinic.staff.doctor</field>
            <field name="prefix">DOC/%(year)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="seq_clinic_staff_dentist" model="ir.sequence">
            <field name="name">Clinic Dentist Code</field>
            <field name="code">clinic.staff.dentist</field>
            <field name="prefix">DEN/%(year)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="seq_clinic_staff_nurse" model="ir.sequence">
            <field name="name">Clinic Nurse Code</field>
            <field name="code">clinic.staff.nurse</field>
            <field name="prefix">NUR/%(year)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="seq_clinic_staff_default" model="ir.sequence">
            <field name="name">Clinic Staff Code</field>
            <field name="code">clinic.staff</field>
            <field name="prefix">STF/%(year)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>