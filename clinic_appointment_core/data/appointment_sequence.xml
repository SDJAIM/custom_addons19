<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Appointment Sequence -->
        <record id="seq_clinic_appointment" model="ir.sequence">
            <field name="name">Clinic Appointment</field>
            <field name="code">clinic.appointment</field>
            <field name="prefix">APT/%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Waiting List Sequence -->
        <record id="seq_clinic_waiting_list" model="ir.sequence">
            <field name="name">Waiting List</field>
            <field name="code">clinic.waiting.list</field>
            <field name="prefix">WL/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>